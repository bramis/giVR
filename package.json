    "babel-preset-react-native": "^1.9.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "react-devtools": "^2.5.2",
    "react-test-renderer": "16.0.0",
